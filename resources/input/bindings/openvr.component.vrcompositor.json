{
  "action_manifest_version": 0,
  "alias_info": {},
  "app_key": "openvr.component.vrcompositor",
  "bindings": {
    "/actions/lasermouse": {
      "poses": [
        {
          "output": "/actions/lasermouse/in/pointer",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/lasermouse/in/pointer",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "path": "/user/hand/left/input/pinch",
          "mode": "button",
          "parameters": {
            "click_activate_threshold": "1",
            "click_deactivate_threshold": "0.9",
            "haptic_amplitude": "0"
          },
          "inputs": {
            "click": {
              "output": "/actions/lasermouse/in/leftclick"
            }
          }
        },
        {
          "path": "/user/hand/left/input/pinch",
          "mode": "button",
          "parameters": {
            "click_activate_threshold": "0.9",
            "click_deactivate_threshold": "0.8",
            "haptic_amplitude": "0"
          },
          "inputs": {
            "click": {
              "output": "/actions/lasermouse/in/lockmouseposition"
            }
          }
        },
        {
          "path": "/user/hand/right/input/pinch",
          "mode": "button",
          "inputs": {
            "click": {
              "output": "/actions/lasermouse/in/leftclick"
            }
          }
        },
        {
          "path": "/user/hand/right/input/pinch",
          "mode": "button",
          "inputs": {
            "click": {
              "output": "/actions/lasermouse/in/lockmouseposition"
            }
          }
        }
      ]
    },
    "/actions/lasermouse_secondary": {
      "sources": [
        {
          "path": "/user/hand/left/input/pinch",
          "mode": "button",
          "inputs": {
            "click": {
              "output": "/actions/lasermouse_secondary/in/switchlaserhand"
            }
          }
        },
        {
          "path": "/user/hand/right/input/pinch",
          "mode": "button",
          "inputs": {
            "click": {
              "output": "/actions/lasermouse_secondary/in/switchlaserhand"
            }
          }
        }
      ]
    },
    "/actions/system": {
      "sources": []
    }
  },
  "category": "steamvr_input",
  "controller_type": "ultraleap_hand",
  "description": "",
  "name": "Default SteamVR Dashboard bindings for Ultraleap hands",
  "options": {
  },
  "simulated_actions": []
}
