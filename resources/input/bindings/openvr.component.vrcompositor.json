{
  "action_manifest_version": 0,
  "alias_info": {},
  "app_key": "openvr.component.vrcompositor",
  "bindings": {
    "/actions/lasermouse": {
      "poses": [
        {
          "output": "/actions/lasermouse/in/pointer",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/lasermouse/in/pointer",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "inputs" : {
            "click" : {
              "output" : "/actions/lasermouse/in/leftclick"
            }
          },
          "mode" : "button",
          "parameters" : {
            "click_activate_threshold" : "0.65",
            "click_deactivate_threshold" : "0.6"
          },
          "path" : "/user/hand/left/input/pinch"
        },
        {
          "inputs" : {
            "click" : {
              "output" : "/actions/lasermouse/in/rightclick"
            }
          },
          "mode" : "button",
          "parameters" : {
            "click_activate_threshold" : "0.65",
            "click_deactivate_threshold" : "0.6"
          },
          "path" : "/user/hand/right/input/pinch"
        }
      ]
    }
  },
  "category": "steamvr_input",
  "controller_type": "ultraleap_hand",
  "description": "",
  "name": "Default SteamVR Dashboard bindings for Ultraleap hands",
  "options": {
  },
  "simulated_actions": []
}
